{
  "name": "gestructureerde-mededeling",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "gestructureerde-mededeling",
  "sourceRoot": "gestructureerde-mededeling/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/gestructureerde-mededeling",
        "index": "gestructureerde-mededeling/src/index.html",
        "main": "gestructureerde-mededeling/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "gestructureerde-mededeling/tsconfig.app.json",
        "assets": [
          "gestructureerde-mededeling/src/favicon.ico",
          "gestructureerde-mededeling/src/assets",
          "gestructureerde-mededeling/src/_redirects"
        ],
        "styles": [
          "normalize.css/normalize.css",
          "@angular/material/prebuilt-themes/indigo-pink.css",
          "gestructureerde-mededeling/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "gestructureerde-mededeling:build:production"
        },
        "development": {
          "browserTarget": "gestructureerde-mededeling:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "deploy": {
      "executor": "@netlify-builder/deploy:deploy",
      "options": {
        "outputPath": "dist/apps/gestructureerde-mededeling",
        "siteId": "1ad9abf9-f9b0-4a9d-9570-50e7a3f7d045"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "gestructureerde-mededeling:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "gestructureerde-mededeling/**/*.ts",
          "gestructureerde-mededeling/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "gestructureerde-mededeling/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
